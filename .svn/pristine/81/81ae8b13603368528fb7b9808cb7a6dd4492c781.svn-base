<view><view class="container"><view class="title"><image class="logo" src="{{heardPicUrl}}"></image><view class="person-info"><text class="userName">周为</text><text class="department">信息技术部（IT主管）</text></view></view><view class="year-container"><uni-icons vue-id="53ba63e8-1" type="back" size="24" bind:__l="__l"></uni-icons><view>{{nowYear+' 年'}}</view><uni-icons vue-id="53ba63e8-2" type="forward" size="24" bind:__l="__l"></uni-icons></view><view class="month-container"><block wx:for="{{monthDataList}}" wx:for-item="monthItem" wx:for-index="index" wx:key="index"><view class="month-list" index="{{index}}" data-event-opts="{{[['tap',[['navigateTo',['$0'],[[['monthDataList','',index,'id']]]]]]]}}" bindtap="__e"><text>{{monthItem.title}}</text><block wx:if="{{monthItem.status=='已确认'}}"><text style="font-size:12px;background-color:#E6F0D7;color:#81B337;border-radius:5px;padding:0px 5px 0px 5px;">{{monthItem.status}}</text></block><block wx:if="{{monthItem.status=='已查看'}}"><text style="font-size:12px;background-color:#E1EFFF;color:#1B84FC;border-radius:5px;padding:0px 5px 0px 5px;">{{monthItem.status}}</text></block><block wx:if="{{monthItem.status=='未生成'}}"><text style="font-size:12px;background-color:#FDF5EC;color:#E99D42;border-radius:5px;padding:0px 5px 0px 5px;">{{monthItem.status}}</text></block><block wx:if="{{monthItem.status=='未查看'}}"><text style="font-size:12px;background-color:#E5E7F8;color:#0014B7;border-radius:5px;padding:0px 5px 0px 5px;">{{monthItem.status}}</text></block><uni-icons vue-id="{{'53ba63e8-3-'+index}}" type="forward" size="18" bind:__l="__l"></uni-icons></view></block></view></view></view>
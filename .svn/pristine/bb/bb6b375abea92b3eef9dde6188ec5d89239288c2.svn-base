<view><view class="uni-margin-wrap"><swiper class="swiper" circular="{{true}}" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><block wx:for="{{swiperList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><swiper-item><navigator class="swiper-item" url="{{'/subpkg/news_details/news_details?news_id='+item.id}}"><image src="{{item.url}}"></image></navigator></swiper-item></block></swiper></view><view class="grid-container"><uni-grid vue-id="8dd740cc-1" column="{{3}}" showBorder="{{false}}" square="{{true}}" data-event-opts="{{[['^change',[['navToDetail']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{gridItemList}}" wx:for-item="gridItem" wx:for-index="index" wx:key="index"><uni-grid-item vue-id="{{('8dd740cc-2-'+index)+','+('8dd740cc-1')}}" index="{{index}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="grid-item-box"><uni-icons vue-id="{{('8dd740cc-3-'+index)+','+('8dd740cc-2-'+index)}}" type="{{gridItem.icon}}" size="30" bind:__l="__l"></uni-icons><text class="text">{{''+gridItem.title+''}}</text></view></uni-grid-item></block></uni-grid></view></view>
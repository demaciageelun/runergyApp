.total-container {
  width: 100%;
  background-color: #F7F8FA;
  display: flex;
  flex-direction: column;
  margin-top: 20px;
  padding-bottom: 20px;
}
.total-container .total-title-container {
  padding-top: 20px;
  margin-bottom: 20px;
  padding-left: 10px;
}
.total-container .total-title-container text {
  font-size: 24px;
  font-weight: 800;
}
.total-container .total-box-container {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  align-items: center;
}
.total-container .total-box-container .box-container {
  width: 17%;
  height: 70px;
  background-color: #fff;
  border-radius: 10px;
}
.total-container .total-box-container .box-container .color-box {
  height: 18%;
  border-radius: 10px 10px 0px 0px;
}
.total-container .total-box-container .box-container .font-box {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 14px;
  color: #6A6A6A;
}
.total-container .total-box-container .box-container .font-box text:first-child {
  font-size: 20px;
  font-weight: 700;
  color: black;
}
.total-container .check-box-container {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}
.total-container .check-box-container .check-container {
  border-radius: 10px;
  background-color: #fff;
  width: 45%;
  height: 80px;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  font-size: 14px;
  color: #6A6A6A;
}
.total-container .check-box-container .check-container text:first-child {
  font-size: 20px;
  font-weight: 700;
  color: black;
}

